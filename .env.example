# Environment variables for AWS Bedrock configuration
# Copy this file to .env and fill in your values

# AWS Configuration
AWS_REGION=<replace with your preffered aws region>
AWS_ACCESS_KEY_ID=<Your Key goes here>
AWS_SECRET_ACCESS_KEY=<Your Secret goes here>

# Optional: If using AWS profiles
AWS_PROFILE=default

# Bedrock Model Configuration
DEFAULT_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1

# Knowledge Graph Configuration
GRAPH_OUTPUT_PATH=./output/
VISUALIZATION_FORMAT=html